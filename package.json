{
    "name": "my-awesome-typescript-project",
    "version": "1.0.1",
    "private": true,
    "description": "Ready to use environment for running and building typescript code.",
    "type": "module",
    "scripts": {
        "list": "npx tsc --project tsconfig.json --listFilesOnly | sort",
        "typecheck": "npx tsc --noEmit --emitDeclarationOnly false && echo \"type checking was successful\"",
        "lint": "npx eslint .",
        "dev": "npx tsx \"src/functional-programming.ts\"",
        "types:build": "npx tsc --noEmit false --emitDeclarationOnly true --noEmitOnError false",        
        "build": "npx babel src --extensions \".ts\" --out-dir \"build\" --ignore \"src/compiler-checks.ts\",\"**/*.d.ts\" --verbose",
        "docs:build": "npx typedoc"
    },
    "dependencies": {
        "core-js": "^3.46.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.28.3",
        "@babel/core": "^7.28.5",
        "@babel/preset-env": "^7.28.5",
        "@babel/preset-typescript": "^7.28.5",
        "@mulekick/eslint-config-muleslint": "^2.2.3",
        "@types/node": "^24.9.2",
        "babel-plugin-transform-import-meta": "^2.3.3",
        "eslint": "^9.38.0",
        "globals": "^16.4.0",
        "tsx": "^4.20.6",
        "typedoc": "^0.28.14",
        "typedoc-plugin-markdown": "^4.9.0",
        "typescript": "^5.9.3"
    },
    "engines": {
        "node": ">=v22.15.0",
        "npm": ">=11.3.0"
    },
    "os": [
        "linux"
    ],
    "author": "mulekick <moolekick@gmail.com> (https://github.com/mulekick)",
    "license": "MIT",
    "repository": {},
    "bugs": {},
    "homepage": "https://github.com/mulekick/typescript-setup"
}
