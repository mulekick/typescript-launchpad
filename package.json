{
    "name": "my-awesome-typescript-project",
    "version": "1.0.0",
    "private": true,
    "description": "Ready to use environment for running typescript code, no transpilation or bundling.",
    "type": "module",
    "scripts": {
        "list": "npx tsc --project tsconfig.json --listFilesOnly | sort",
        "typecheck": "npx tsc --project tsconfig.json --noEmit --emitDeclarationOnly false && echo 'type checking was successful'",
        "lint": "npx eslint .",
        "dev": "npx tsx ./src/main.ts",
        "docs:build": "npx typedoc"
    },
    "dependencies": {},
    "devDependencies": {
        "@mulekick/eslint-config-muleslint": "^2.0.6",
        "@types/node": "^22.8.1",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "eslint": "^8.57.1",
        "globals": "^13.24.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "tsx": "^4.19.2",
        "typedoc": "^0.27.4",
        "typedoc-plugin-markdown": "^4.3.2",
        "typescript": "^5.6.3"
    },
    "engines": {
        "node": ">=20.17.0",
        "npm": ">=10.8.2"
    },
    "os": [
        "linux"
    ],
    "author": "mulekick <moolekick@gmail.com> (https://github.com/mulekick)",
    "license": "MIT",
    "repository": {},
    "bugs": {},
    "homepage": "https://github.com/mulekick/typescript-setup"
}
